{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React Starter Project",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix src e2e/specs e2e/pageobjects",
    "start:dev": "NODE_ENV=development webpack-dev-server",
    "test": "NODE_ENV=test jest",
    "test:update": "npm run test -- -u"
  },
  "author": "Vicent Gozalbes <vigosan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "prettier": "^1.2.2",
    "react-test-renderer": "^15.5.4",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "semi": false
        }
      ]
    }
  },
  "jest": {
    "moduleDirectories": ["node_modules", "src"],
    "testPathIgnorePatterns": [
      "/e2e/"
    ]
  }
}
